{
  "expo": {
    "name": "DevGuides",
    "slug": "DevGuides",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon":"app/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "image":"app/assets/images/icon.png",
      "backgroundColor": "#1f1f1f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.devguides.app",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      },
      "icon":"app/assets/images/icon.png"
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.devguides.app"
    },
    "plugins":[
      [
        "expo-notifications",{
         "enableBackgroundRemoteNotifications":true
        }
      ]
    ],
    "web": {},
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {
            }
          }
        },
        "projectId": "3a17d314-84d0-49f9-8373-f3eee01578a5"
      }
    }
  }
}
