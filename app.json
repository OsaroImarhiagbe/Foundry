{
  "expo": {
    "name": "DevGuides",
    "slug": "DevGuides",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "image": "./assets/images/people.png",
      "backgroundColor": "#1f1f1f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "icon": "./assets/images/icon.png",
      "bundleIdentifier": "com.devguides.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "fetch"
        ]
      }
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.devguides.app"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "web": {},
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {}
          }
        },
        "projectId": "3a17d314-84d0-49f9-8373-f3eee01578a5"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/3a17d314-84d0-49f9-8373-f3eee01578a5"
    }
  }
}
