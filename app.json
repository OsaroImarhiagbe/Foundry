{
  "expo": {
    "name": "DevGuides",
    "slug": "DevGuides",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.devguides.app"
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.devguides.app"
    },
    "web": {},
    "plugins": [
      [
        "expo-notfications",
        {
          "mode": "development",
          "devTeam": "UFQYWBYAJF"
        }
      ]
    ],
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.devguides.app.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.devguides.app.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "3a17d314-84d0-49f9-8373-f3eee01578a5"
      }
    }
  }
}
