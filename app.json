{
  "expo": {
    "name": "DevGuides",
    "slug": "DevGuides",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.yourname.devguides"
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "plugins": [
      [
        "onesignal-expo-plugin",
      {
        "mode": "development"
      }
    ]
    ]
  }
}
