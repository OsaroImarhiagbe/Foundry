      "bundleIdentifier": "com.devguides.app"
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.devguides.app"
    },
    "web": {},
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development",
          "devTeam": "UFQYWBYAJF"
        }
      ]
    ]
  }
}
