{
  "expo": {
    "name": "BlueJay",
    "slug": "BlueJay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "image": "./app/assets/images/icon.png",
      "backgroundColor": "#1f1f1f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true,
      "newArchEnabled": true,
      "icon": "./app/assets/images/icon.png",
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.devguides.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "fetch"
        ]
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/messaging",
      "@react-native-firebase/crashlytics",
      "@react-native-google-signin/google-signin",
      [
        "expo-notifications",
        {
          "icon": "./app/assets/images/icon.png",
          "color": "#ffffff",
          "useNextNotificationsApi": true
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "web": {},
    "extra": {
      "eas": {
        "projectId": "4fa4e2f4-26f7-48b0-96e6-cd88eb8e2c16"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/3a17d314-84d0-49f9-8373-f3eee01578a5"
    },
    "android": {
      "package": "com.devguides.app"
    }
  }
}
